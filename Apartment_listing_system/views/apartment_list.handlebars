<main id="main">
	<section class="blog">
		<div class="container">
			<div class="row">
				<div class="col-lg-12 mb-4 text-right">
					<a href="./apartment/add" class="btn btn-secondary"><i class="icofont-plus"></i> New Add</a>
				</div>
				<div class="col-lg-12">
					<div class="table-responsive" id="aepartment-table-wrapper">
						<table id="apartment-table" class="display" style="overflow:scroll; width: 100%;">
							<thead>
								<tr>
									<th>No</th>
									<th>Title</th>
									<th>Utilities</th>
									<th>Price</th>
									<th>Address</th>
									<th>Action</th>
								</tr>
							</thead>
							<tbody>
							{{#each apartments}}
								<tr>
									<td>{{listing_id}}</td>
									<td>{{title}}</td>
									<td>{{utilities_included}}</td>
									<td>${{price}}</td>
									<td>{{address}}</td>
									<td>{{listing_id}}</td>
								</tr>
							{{/each}}
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</section>
</main>